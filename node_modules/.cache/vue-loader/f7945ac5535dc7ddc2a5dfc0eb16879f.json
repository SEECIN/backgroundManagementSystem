   <el-table :data=\"tableData\"\r\n                border\r\n                stripe\r\n                :max-height=\"maxHeight\">\r\n        <el-table-column v-for=\"item,idx in tableHeadData\"\r\n                         :key=\"idx\"\r\n                         :label=\"item.name\">\r\n          <template slot-scope=\"scope\">\r\n            <span v-if=\"item.type==='text'\">{{scope.row[item.field] || '/'}}</span>\r\n            <el-tag v-if=\"item.type==='tag'\"\r\n                    :type=\"scope.row.tagData.type\">{{scope.row.tagData.text}}</el-tag>\r\n            <el-switch v-if=\"item.type==='switch'\"\r\n                       v-model=\"scope.row.status\"\r\n                       :active-value=\"1\"\r\n                       :inactive-value=\"0\"\r\n                       @change=\"handleChangeSwitch(scope.row.status,scope.row,item.messageType,item.axiosType)\"\r\n                       active-color=\"#13ce66\"\r\n                       inactive-color=\"#ff4949\">\r\n            </el-switch>\r\n            <template v-if=\"item.type==='button'\">\r\n              <el-button v-for=\"curr,id in item.field\"\r\n                         :key=\"id\"\r\n                         :type=\"curr.type\"\r\n                         :icon=\"curr.icon\"\r\n                         @click=\"hanldeClickButton(curr.axiosType,scope.row)\"></el-button>\r\n            </template>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </template>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MIXIN from '@/core/mixin'\r\nimport message from '@/plugins/message'\r\nimport TABLE_HEAD_DATA from '@/config/tableHead.config.js'\r\nimport MESSAGE_DATA from '@/config/message.config.js'\r\nexport default {\r\n  name: 'Table',\r\n  data() {\r\n    return {\r\n      tableHeadData: []\r\n    }\r\n  },\r\n  mixins: [MIXIN],\r\n  props: {\r\n    tableData: {\r\n      type: Array\r\n    }\r\n  },\r\n  created() {\r\n    this.tableHeadData = TABLE_HEAD_DATA[this.$route.name]\r\n  },\r\n  computed: {\r\n    maxHeight() {\r\n      return (window.innerHeight - 310) + 'px'\r\n    }\r\n  },\r\n  methods: {\r\n    async handleChangeSwitch(status, item, messageType, type) {\r\n      let msgInfo = MESSAGE_DATA[messageType][Number(status)].message\r\n      message(msgInfo, item, async () => {\r\n        try {\r\n          await this.$axios(type, item)\r\n        } catch (error) {\r\n          console.log(error);\r\n        }\r\n      })\r\n    },\r\n    async hanldeClickButton(type, item) {\r\n      if (type.includes('edit')) {\r\n        this.$bus.$emit('transmitFormData', item)\r\n        this.openModal(type)\r\n      }\r\n      if (type.includes('delete')) {\r\n        try {\r\n          await this.$axios(type, item)\r\n          this.$bus.$emit('changeData', item, type)\r\n        } catch (error) {\r\n          console.log(error)\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"stylus\" scoped>\r\n.table\r\n  height 100%\r\n  margin 30px 0\r\n  .el-table\r\n    height: calc(100% - 600px)\r\n</style>\r\n"]}]}